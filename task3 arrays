// Создаем массив styles с элементами «Джаз» и «Блюз»
let styles = ['Джаз', 'Блюз'];

// Добавляем «Рок-н-ролл» в конец
styles.push('Рок-н-ролл');
console.log(styles); // Джаз, Блюз, Рок-н-ролл

// Заменяем значение в середине на «Классика»
const mid = Math.floor(styles.length / 2);
styles[mid] = 'Классика';
console.log(styles); // Джаз, Классика, Рок-н-ролл

// Удаляем первый элемент массива и показываем его
const first = styles.shift();
console.log(first); // Джаз
console.log(styles); // Классика, Рок-н-ролл

// Вставляем «Рэп» и «Регги» в начало массива
styles.unshift('Рэп', 'Регги');
console.log(styles); // Рэп, Регги, Классика, Рок-н-ролл
