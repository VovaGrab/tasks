//С оератором
let age = 10;

if (!(age >= 14 && age <= 90)) {
    // Ваш код, который выполнится, если age НЕ находится в диапазоне 14 и 90 включительно
}

//Без оератора
let age = 10;

if (age < 14 || age > 90) {
    // Ваш код, который выполнится, если age НЕ находится в диапазоне 14 и 90 включительно
}
